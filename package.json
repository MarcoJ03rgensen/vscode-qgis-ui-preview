{
  "name": "vscode-qgis-ui-preview",
  "displayName": "QGIS / Qt UI Preview",
  "description": "Preview PyQt .ui files and compiled .py GUI files for QGIS plugin development directly in VS Code",
  "version": "0.0.3",
  "publisher": "MarcoJ03rgensen",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarcoJ03rgensen/vscode-qgis-ui-preview.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qgis-ui.preview",
        "title": "Preview QGIS UI",
        "icon": "$(open-preview)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "qgis-ui.preview",
          "when": "resourceExtname == '.ui' || resourceExtname == '.py'",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "qgis-ui.preview",
          "when": "resourceExtname == '.ui' || resourceExtname == '.py'",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "qgis-ui.preview",
          "when": "resourceExtname == '.ui' || resourceExtname == '.py'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "18.x",
    "typescript": "^5.1.3"
  }
}
